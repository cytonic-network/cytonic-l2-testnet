{
    "parent-chain": {
        "connection": {
            "url": "http://host.docker.internal:8545"
        }
    },
    "chain": {
        "id": 52226,
        "name": "cytonic-l2-testnet",
        "info-files": [
            "/config/l2_chain_info.json"
        ]
    },
    "node": {
        "staker": {
            "dangerous": {
                "without-block-validator": false
            },
            "parent-chain-wallet": {
                "private-key": ""
            },
            "disable-challenge": false,
            "enable": false,
            "staker-interval": "10s",
            "make-assertion-interval": "10s",
            "strategy": "MakeNodes"
        },
        "sequencer": true,
        "dangerous": {
            "no-sequencer-coordinator": true,
            "disable-blob-reader": true
        },
        "delayed-sequencer": {
            "enable": true,
            "use-merge-finality": false,
            "require-full-finality": false
        },
        "batch-poster": {
            "enable": true,
            "redis-url": "redis://redis:6379",
            "max-delay": "30s",
            "l1-block-bound": "ignore",
            "parent-chain-wallet": {
                "private-key": ""
            },
            "data-poster": {
                "redis-signer": {
                    "signing-key": ""
                },
                "wait-for-l1-finality": false
            }
        },
        "block-validator": {
            "validation-server": {
                "url": "ws://validation-node:8549",
                "jwtsecret": "/config/val_jwt.hex"
            }
        },
        "data-availability": {
            "enable": false
        }
    },
    "execution": {
        "sequencer": {
            "enable": true
        },
        "forwarding-target": "null"
    },
    "persistent": {
        "chain": "local"
    },
    "ws": {
        "addr": "0.0.0.0"
    },
    "http": {
        "addr": "0.0.0.0",
        "vhosts": "*",
        "corsdomain": "*"
    },
    "file-logging": {
        "enable": false
    }
}